    "data": "{\"color\":\"default\",\"parent\":[{\"annotations\":{\"bold\":true,\"italic\":false,\"strikethrough\":false,\"underline\":false,\"code\":false,\"color\":\"default\"},\"plain_text\":\"Linux（Ubuntu）の場合はDocker Engine（cli）をインストールします\",\"href\":null}]}",
