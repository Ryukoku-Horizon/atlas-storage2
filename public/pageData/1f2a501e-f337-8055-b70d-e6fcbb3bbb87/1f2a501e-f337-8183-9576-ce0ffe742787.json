    "data": "{\"language\":\"python\",\"parent\":[\"import flet as ft\\n\\ndef main(page: ft.Page):\\n    con1 = ft.Column(\\n        controls=[\\n            ft.Container(\\n                width=150,\\n                height=150,\\n                margin=20,\\n                bgcolor=ft.colors.GREEN,\\n                border_radius=20,\\n            ),\\n            ft.Text(\\\"width=150\\\\nheight=150\\\\nmargin=20\\\\nbgcolor=ft.colors.GREEN\\\\nborder_radius=20\\\")\\n        ],\\n        horizontal_alignment=ft.CrossAxisAlignment.CENTER\\n    )\\n\\n    con2 = ft.Column(\\n        controls=[\\n            ft.Container(\\n                padding=50,\\n                margin=20,\\n                content=ft.Text(\\\"Googleが開きます\\\"),\\n                bgcolor=\\\"BLUE\\\",\\n                url = \\\"https://www.google.com/\\\",\\n            ),\\n            ft.Text('padding=50\\\\nmargin=20\\\\ncontent=ft.Text(\\\"Googleが開きます\\\")\\\\nbgcolor=\\\"BLUE\\\"\\\\nurl = \\\"https://www.google.com/\\\"')\\n        ],\\n        horizontal_alignment=ft.CrossAxisAlignment.CENTER\\n    )\\n\\n    con3 = ft.Column(\\n        controls=[\\n            ft.Container(\\n                padding=20,\\n                margin=20,\\n                content=ft.Column(\\n                    controls=[\\n                        ft.Icon(ft.icons.PETS),\\n                        ft.Text(\\\"複数コントロールも設置可能！\\\"),\\n                        ft.Icon(ft.icons.ROCKET),\\n                    ],\\n                    horizontal_alignment=ft.CrossAxisAlignment.CENTER\\n                ),\\n                bgcolor=ft.colors.RED,\\n            ),\\n            ft.Text(\\\"padding=20\\\\nmargin=20\\\\ncontent=中略・以下コードを参照\\\\nbgcolor=ft.colors.RED\\\")\\n        ],\\n        horizontal_alignment=ft.CrossAxisAlignment.CENTER\\n    )\\n\\n    row = ft.Row(\\n        controls=[\\n            con1,\\n            con2,\\n            con3\\n        ]\\n    )\\n\\n    page.add(row)\\n\\nft.app(target=main)\"],\"caption\":[]}",
