    "parentId": "1f2a501e-f337-816b-a4d3-f52c8d002e24",
    "data": "{\"language\":\"python\",\"parent\":[\"from turtle import bgcolor\\nimport flet as ft\\n\\ndef main(page: ft.Page):\\n    page.title = \\\"ボタンのカスタマイズ\\\"\\n\\n    cupertino_button = ft.Column(\\n        controls=[\\n            ft.Text(\\\"ft.CupertinoButton\\\"),\\n            ft.CupertinoButton(\\n                text=\\\"ボタンです\\\",\\n                height=200,\\n                width=70,\\n                padding=0,\\n                border_radius=0,\\n                bgcolor=ft.colors.PURPLE_400,\\n                color=ft.colors.YELLOW_400,\\n            ),\\n            ft.Text(value = 'text=\\\"ボタンです\\\"\\\\nheight=200\\\\nwidth=70\\\\npadding=0\\\\nborder_radius=0\\\\nbgcolor=ft.colors.PURPLE_400\\\\ncolor=ft.colors.YELLOW_400')\\n        ],\\n        horizontal_alignment=ft.CrossAxisAlignment.CENTER\\n    )\\n\\n    elevated_button = ft.Column(\\n        controls=[\\n            ft.Text(\\\"ft.ElevatedButton\\\"),\\n            ft.ElevatedButton(\\n                content=ft.Column(\\n                    controls=[\\n                        ft.Text(value=\\\"ボタンの中に\\\", size=20),\\n                        ft.Text(\\\"いろいろ置けます\\\"),\\n                        ft.Row(\\n                            controls=[\\n                                ft.Icon(name=ft.icons.FAVORITE, color=\\\"pink\\\"),\\n                                ft.Icon(name=ft.icons.AUDIOTRACK, color=\\\"green\\\"),\\n                                ft.Icon(name=ft.icons.BEACH_ACCESS, color=\\\"blue\\\"),\\n                            ]\\n                        )\\n                    ]\\n                )\\n            ),\\n            ft.Text(value = 'content=ft.Column(中略、\\\\n    下のコードを参照)')\\n        ],\\n        horizontal_alignment=ft.CrossAxisAlignment.CENTER\\n    )\\n\\n    filled_tonal_button = ft.Column(\\n        controls=[\\n            ft.Text(\\\"ft.FilledTonalButton\\\"),\\n            ft.FilledTonalButton(\\n                icon=ft.icons.LOCK,\\n                text=\\\"Bt\\\",\\n                disabled=True,\\n                tooltip=\\\"このボタンは無効です.\\\",\\n            ),\\n            ft.Text(value = 'icon=ft.icons.LOCK\\\\ntext=\\\"Bt\\\"\\\\ndisabled=True\\\\ntooltip=\\\"このボタンは無効です.\\\"\\\\n')\\n        ],\\n        horizontal_alignment=ft.CrossAxisAlignment.CENTER\\n    )\\n\\n    row1 = ft.Row(\\n        controls=[\\n            cupertino_button,\\n            elevated_button,\\n            filled_tonal_button,\\n        ],\\n        alignment=ft.MainAxisAlignment.SPACE_EVENLY\\n    )\\n\\n    page.add(row1)\\n\\nft.app(target=main)\\n\"],\"caption\":[]}",
